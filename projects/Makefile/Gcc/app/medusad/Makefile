#
medusad_USRCXXFLAGS += \
$(medusa_USRCXXFLAGS) \

#
# user c flags
#
medusad_USRCFLAGS += \
$(medusa_USRCFLAGS) \

#
# user ld flags
#
medusad_USRLDFLAGS += \
